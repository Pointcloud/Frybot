<launch>
  <arg name="robot_name"   default="robotis_manipulator_h"/>
  <arg name="end_effector" default="gripper"/>
  <arg name="with_gripper" default="true"/>  

  <group ns="$(arg robot_name)">
    <node name="teleop_keyboard" pkg="frystation" type="teleop_keyboard" output="screen">
      <remap from="kinematics_pose" to="$(arg end_effector)/kinematics_pose"/>
      <param name="end_effector_name" value="$(arg end_effector)"/>
      <param name="with_gripper"      value="$(arg with_gripper)"/>
    </node>
  </group>
</launch>